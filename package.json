{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite --host",
    "preview": "vite preview",
    "build": "vue-tsc && vite build",
    "typing": "typechain --target=truffle-v5 src/assets/eth/build/*.json --out-dir src/assets/types/truffle && typechain --target=ethers-v5 src/assets/eth/build/*.json --out-dir src/assets/types/ethers",
    "compile": "truffle compile --config truffle-config.cjs && npm run typing",
    "migrate": "truffle migrate --config truffle-config.cjs"
  },
  "dependencies": {
    "@varlet/ui": "^2.9.2",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.14",
    "default-passive-events": "^2.0.0",
    "ethers": "^5.7.2",
    "identicon.js": "^2.3.3",
    "pinia": "^2.0.28",
    "pinia-plugin-persist": "^1.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4",
    "vue": "^3.2.45",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.39",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/truffle-v5": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "truffle": "^5.8.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.1",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^1.0.11"
  }
}
